package Modele;

public class Personne {
	
	String nom;
	String salaire;
	public String getNom() {
		return nom;
	}
	public void setNom(String nom) {
		this.nom = nom;
	}
	public String getSalaire() {
		return salaire;
	}
	public void setSalaire(String salaire) {
		this.salaire = salaire;
	}
	
	public String toString() { return getNom() +""+ getSalaire(); }
	
	

}
